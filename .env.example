# KRATOS Configuration
KRATOS_VERSION=1.0.0
KRATOS_ENV=development

# Azure Configuration
AZURE_SUBSCRIPTION_ID=your_subscription_id_here
AZURE_TENANT_ID=your_tenant_id_here
AZURE_CLIENT_ID=your_client_id_here
AZURE_CLIENT_SECRET=your_client_secret_here
AZURE_RESOURCE_GROUP=your_resource_group_here
AZURE_AKS_CLUSTER_NAME=your_aks_cluster_name_here

# MCP Server Configuration
MCP_SERVER_HOST=localhost
MCP_SERVER_PORT=8080
AKS_MCP_ENDPOINT=http://localhost:3000

# AutoGen Configuration
AUTOGEN_CACHE_SEED=42
AUTOGEN_MAX_ROUND=10
AUTOGEN_TEMPERATURE=0.1

# Streamlit Configuration
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=0.0.0.0

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Security
KRATOS_API_KEY=your_secure_api_key_here
ENCRYPT_SENSITIVE_DATA=true